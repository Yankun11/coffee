<view class="banner">
	<swiper interval="{{interval}}" duration="{{duration}}" bindchange="swiperChange">
		<swiper-item wx:for="{{swiperArr}}" wx:key="*this">
			<image src="{{item}}" mode="widthFix"></image>
		</swiper-item>
	</swiper>
	<view class="dots">
		<view wx:for="{{swiperArr}}" wx:key="*this" class="{{activeNum==index ? 'active':''}}"></view>
	</view>
</view>
